
<ThirdWindowHome>:
    name:'third_home'
    id: third_window_home
    FloatLayout:
        orientation: 'vertical'
        MDIconButton:
            name: 'Back'
            text: 'Back'
            icon: 'chevron-left'
            pos:"10dp", "550dp"
            on_release:
                print(app.root.ids.first_main_window.ids.home_screen_manager.current)
                app.root.current = 'first_main_window'
                app.root.ids.first_main_window.ids.home_screen_manager.current = 'first_home'
                print('root.parent: ', root.parent)

                root.manager.transition.direction = 'left'
    BoxLayout:
        padding: 10
#        ImageButton:
        Button:
            background_color: 0, 0, 0, 0
            size_hint: 0.5, 1
            text: 'Start'
            color: 0, 0, 0, 1
            canvas.before:
                Color:
                    rgba: 0.96, 0.68, 0.13, 1
                RoundedRectangle:
                    radius: [15,]
                    size: self.size
                    pos: self.pos
            font_size: 25
            bold: True
            pos_hint: {'x':0, 'y':0}
            id: but
#            source:'image/start.png'

            size_hint: 0.2, 0.1
            mipmap: True
#            pos_hint: {'center_x': .5, 'center_y': .5}
            on_press:
                app.root.current = 'fourth_home'
                app.root.ids.fourth_window_home.ids.videoo.state = 'play'
                app.root.ids.fourth_window_home.build()
                app.root.ids.fourth_window_home.pris()
